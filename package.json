{
  "name": "react-webpack-typescript-starter-rspack",
  "version": "0.1.0",
  "description": "React rspack example comparing with webapck",
  "keywords": [
    "react",
    "rspack",
    "webpack",
    "typescript",
    "sass",
    "hmr"
  ],
  "author": "Pravanjan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pravanjan/react-webpack-typescript-starter-rspack.git"
  },
  "bugs": {
    "url": "https://github.com/pravanjan/react-webpack-typescript-starter-rspack/issues"
  },
  "homepage": "https://github.com/pavanjan/react-webpack-typescript-starter-rspack",
  "scripts": {
    "build": "yarn run clean-dist && webpack --config=configs/webpack/prod.js",
    "build:rspack": "yarn run clean-dist && rspack  -c rspack.config.js",
    "clean-dist": "rimraf dist/*",
    "lint": "eslint './src/**/*.{js,ts,tsx}' --quiet",
    "start:webpack": "webpack serve -c configs/webpack/dev.js ",
    "start:rspack": "rspack serve --mode=development",
    "start:prod": "yarn run build && node express.js",
    "test": "jest --coverage --watchAll --config=configs/jest.json"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@rspack/cli": "^0.1.11",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "express": "^4.18.2",
    "fork-ts-checker-webpack-plugin": "^7.3.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-refresh": "^0.14.0",
    "rimraf": "^4.1.2",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0",
   "html-webpack-plugin": "^5.5.3"

  }
}
